{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": ["./"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/*)"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "./function-documentation",
    "recurse": true,
    "readme": "./README.md"
  }
}
